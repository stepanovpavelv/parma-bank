{
  "name": "sink-connector",
  "config": {
    "connector.class": "io.confluent.connect.jdbc.JdbcSinkConnector",
    "tasks.max": "2",
    "connection.url": "jdbc:postgresql://report_db:5433/bank_reports",
    "connection.user": "reporting_sa",
    "connection.password": "sa123",
    "topics": "bankmoney.public.operation_types,bankmoney.public.operation_statuses,bankmoney.public.bank_accounts,bankmoney.public.account_details,bankmoney.public.operations,bankmoney.public.operation_status_history",
    "pk.fields": "id",
    "pk.mode": "record_key",
    "auto.create": "false",
    "auto.evolve": "false",
    "insert.mode": "upsert",
    "delete.enabled": "true",
    "transforms": "route,unwrap,ts_date,ts_actual_date,ts_create_date,ts_update_date",
    "transforms.route.type": "org.apache.kafka.connect.transforms.RegexRouter",
    "transforms.route.regex": "([^.]+)\\.([^.]+)\\.([^.]+)",
    "transforms.route.replacement": "bank_reports.public.$3",
    "transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState",
    "transforms.unwrap.drop.tombstones": "false",
    "transforms.ts_date.type": "org.apache.kafka.connect.transforms.TimestampConverter$Value",
    "transforms.ts_date.target.type": "Timestamp",
    "transforms.ts_date.field": "date",
    "transforms.ts_date.format": "yyyy-MM-dd'T'HH:mm:ss",
    "transforms.ts_actual_date.type": "org.apache.kafka.connect.transforms.TimestampConverter$Value",
    "transforms.ts_actual_date.target.type": "Timestamp",
    "transforms.ts_actual_date.field": "actual_date",
    "transforms.ts_actual_date.format": "yyyy-MM-dd'T'HH:mm:ss",
    "transforms.ts_create_date.type": "org.apache.kafka.connect.transforms.TimestampConverter$Value",
    "transforms.ts_create_date.target.type": "Timestamp",
    "transforms.ts_create_date.field": "create_date",
    "transforms.ts_create_date.format": "yyyy-MM-dd'T'HH:mm:ss",
    "transforms.ts_update_date.type": "org.apache.kafka.connect.transforms.TimestampConverter$Value",
    "transforms.ts_update_date.target.type": "Timestamp",
    "transforms.ts_update_date.field": "update_date",
    "transforms.ts_update_date.format": "yyyy-MM-dd'T'HH:mm:ss"
  }
}